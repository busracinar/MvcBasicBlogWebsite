@model IEnumerable<BlogMvcApp.Models.Category>


    <div class="well">
        <h4>Kategoriler</h4>
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-lg-6">
                    <ul class="list-unstyled">
                        <li>
                            <a href="/Blog/List/@item.Id">@item.KategoriAdi</a>
                        </li>
                    </ul>
                </div>
            }

        </div>
    </div>